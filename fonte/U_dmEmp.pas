function TdmEmp.chkPer( formu, mods: String): Boolean;
var
cSql: String;
SqlP : TFdQuery;
Begin
    SqlP := TFdQuery.Create(nil);

  cSql:='Select * From  "dbconfig".Permissao where prm_CODUSU = '+inttoStr(sysCodusu )
        +' and prm_formda = '''+formu+''' and '+mods+' = ''T''';

  with SqlP do
    begin
      Close;
      Connection := SqlConSis;
      SQL.Text:= cSql;
      open;
    end;

    if SqlP.Eof then
       begin
         Result:=False
       end Else Begin
         Result:=True
       End;


End;



procedure TdmEmp.SqlConSisAfterConnect(Sender: TObject);
Var
TConn : TConecte;
cSql  : string;
qQry  : TFDQuery ;
begin
cSql  := sqlEmpresa.SQL.Text ;

  TConn := TConecte.GetInstance;
  if SqlConSis.Connected then begin
     TConn.SchemaCFG(SqlConSis);
     // Verifica se existe alguma empresa inserida no banco
     qQry  := TFDQuery.Create(Nil) ;
     with qQry do begin
        Connection := SqlConSis ;
        SQL.Text := cSql ;
        open;
     end;
     if qQry.Eof then begin
        frmEmpresaInc := TfrmEmpresaInc.create(Self);
        frmEmpresaInc.ShowModal;
        with qQry do begin
            Close;
            Connection := SqlConSis ;
            SQL.Text   := cSql ;
            open;
        end;
        if qQry.Eof then begin
           dmEmp.SqlConSis.Connected := False;
           Application.Terminate;
        end;
     end;
     FreeAndNil(qQry);
  end Else Begin
     MessageDlg('Conexo no estabelecida', mtError,[mbYes],0);
     Application.Terminate;
  End;
end;

procedure TdmEmp.SqlConSisBeforeConnect(Sender: TObject);
Var
  inuF: TINIFILE;
  TCon : TConecte;
begin
  variaveis;
  //inuF := TIniFile.Create(CamIni+ArqIni);
  if Not FileExists(CamIni+ArqIni) then begin
      frmCriadb :=  TfrmCriadb.Create(Nil);
      frmCriadb.ShowModal ;
  end ELse
      LeIni('R');

  TCon := TConecte.GetInstance;
  try
    //- Connection parameters
    if tcon.ExistDatabase( adm_TipoCnx,adm_hostcnx, adm_nmBcocnx, adm_usuariocnx, adm_passcnx, adm_libcnx  ) then begin
       tcon.Conectar(SqlConSis, adm_TipoCnx,adm_hostcnx, adm_nmBcocnx, adm_usuariocnx, adm_passcnx  );
    End Else begin
       ShowMessage('Erro na Gerao do Banco');
       Application.Terminate;
    end;
  except on e : exception do
    showmessage('Ocorreu o seguinte erro: '+ e.message);
  end;

end;

procedure TdmEmp.DataModuleCreate(Sender: TObject);
begin
  try
    SqlConSis.Connected := True;
  except on e : exception do
    showmessage('Ocorreu o seguinte erro: '+ e.message);
  end;
end;

